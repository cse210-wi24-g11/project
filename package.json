{
  "name": "cse210-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:eslint": "eslint . --ext ts,tsx --ignore-path .gitignore --report-unused-disable-directives --max-warnings 0",
    "lint:prettier": "prettier --check '**/*.{ts,tsx,html,css,yml,json}'",
    "format": "npm run format:prettier && npm run format:eslint",
    "format:eslint": "npm run lint:eslint -- --fix",
    "format:prettier": "npm run lint:prettier -- --write",
    "test": "vitest --coverage.enabled true",
    "preview": "vite preview"
  },
  "dependencies": {
    "@adobe/react-spectrum": "^3.34.1",
    "@react-spectrum/toast": "^3.0.0-beta.10",
    "@spectrum-icons/workflow": "^4.2.10",
    "@types/d3": "^7.4.3",
    "react": "^18.2.0",
    "react-aria": "^3.32.1",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.1",
    "react-svg": "^16.1.33"
  },
  "devDependencies": {
    "@tailwindcss/line-clamp": "^0.4.4",
    "@types/react": "^18.2.55",
    "@types/react-color": "^3.0.12",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.2.2",
    "autoprefixer": "^10.4.17",
    "d3": "^7.8.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "react-calendar": "^4.8.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vite-plugin-pwa": "^0.18.2",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.2"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.12.1"
  }
}
